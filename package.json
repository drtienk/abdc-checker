{
  "name": "abdc-checker",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "generate:data": "python3 scripts/generate_abdc_json.py",
    "predev": "npm run generate:data",
    "dev": "vite",
    "prebuild": "npm run generate:data",
    "build": "vite build",
    "preview": "vite preview",
    "check": "python3 scripts/generate_abdc_json.py && python3 -m json.tool public/data/abdc.json >/dev/null && node --check src/main.js"
  },
  "devDependencies": {
    "vite": "^5.4.10"
  }
}
